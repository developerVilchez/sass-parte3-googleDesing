doctype html
html(lang="es")
	head
		title Google Desing
		meta(charset="utf-8")
		meta(name="viewport" content="width=device-width,initial-scale=1")
		link(rel="stylesheet", href="assets/css/estilos.css")
		script(src="https://use.fontawesome.com/9470d83244.js")
	body
		header#header
			.grupo
				.caja
					.header-main
						.header-main-menu
							a.header-main__menu(href="#")
								i.fa.fa-bars(aria-hidden="true")

						.header-main-banner
							img(src="assets/img/Google_2015_logo.svg.png", alt="logo_google" class="header-main-banner__logo")
							span.header-main-banner__title Desing

						.header-main-search
							a.header-main__search(href="#")
								i.fa.fa-search(aria-hidden="true")
		section#podcast
			.grupo
				.caja
					.section-podcast
						.section-podcast__banner
							img(src="assets/img/podcast.png", alt="banner_podcast" class="section-podcast__banner section-podcast__banner__img")
						h1.section-podcast__title Method Podcast, Episode 1
						p.section-podcast__paragraph Prototyping, tools, and designer who code: A conversation with Jay Wong of Project Fi
						p.section-podcast__article Hosted by Interaction Designer Aidan Simpson, the Method podcast profiles designers at Google, giving listeners an inside look at their journeys and desing choices. In ...
						span.section-podcast__note Profile
		div.divider
			.grupo
				.caja
					.modulo-divider
						hr.modulo-divider__hr
		section#publications
			.grupo
				.caja
					.section-publications
						.section-publications-header
							h2.section-publications-header__title Newsfeed
							a.section-publications-header__link(href="#") More News
				.caja
					.section-publications
						.section-publications-article
							span.section-publications-article__date 06/13/2017
							h2.section-publications-article__title A new and improved Google Design
							img.section-publications-article__img(src="assets/img/newDesing.jpg")
							


		script(src="http://code.jquery.com/jquery-1.11.2.min.js" charset="utf-8")
	